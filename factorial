def factorial(n):
    
    c=1
    for i in range(1,n+1):
        c=c*i
    return(c)
    #this code is for finding the factorial
    
    
    
    
    
    
    
    #this code is for finding factorial without recursion
    def factorial(a):
    if a==1:
        return 1
    else:
        return factorial(a-1)*a
